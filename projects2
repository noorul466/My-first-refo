##==>16

def baniry(input_str):
    for i in input_str:
        if i not in '101': 
            return False       
    return True    


input_str='1000110'
if baniry(input_str):
    print(f"{input_str}")


##==> 17

def add(*args):
    total=0
    for i in args:
        total +=1
    return total

print(add(1,2,3,4,5,6,7))        

##===>18
def key_words(**kwargs):
    for key,value in kwargs.items():
        print(f"{key}  = {value}")

key_words(name="noorul",age=23)        


