if num <= 0:
    print("please Enter positive number")
else:
    result=math.log(num)
    print(f"natural nunber is log {num} result is {result}")


##==>11
def find_largesrt_element(arr):
    if not arr:
        return "Arry is empty" 
        largest_element=arr[0]
    for i in arr:
        if i > largest_element:
            largest_element=i
    return arr    


my_array=[12,22,67,90,27,3,2]
result=find_largesrt_element(my_array)
print(f"the largest element is {result}") 


