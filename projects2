numbers=[1,2,-3,-45,67,89,90]
minimum=numbers[0]
max=numbers[0]

for i in numbers:
    if i < minimum:
        minimum =i
print(minimum) 


for i in numbers:
    if i >max:
        max =i 

print(max)        


